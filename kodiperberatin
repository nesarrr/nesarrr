from instapy import InstaPy

session = InstaPy(username="mr.gravedigger2003", password="nesfun171819!!!")
session.login()


session.set_do_comment(enabled=True, percentage=100)
session.set_comments(['Nice!', 'awesome', 'hey', 'nice post'])

